<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Tampilan User</title>
</head>
<body>
    <a href="/register">Tambah User</a>
    <table border="1">
        <thead>
            <th>Nama Lengkap</th>
            <th>Email</th>
            <th>Password</th>
        </thead>

        <tbody>
            <% users.forEach((user) => { %>
                <tr>
                    <td><%= user.nama %></td>
                    <td><%= user.email %></td>
                    <td><%= user.password %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>